<% title "Photos", :h1 %>

<table>
  <% for photo in @photos %>
    <tr>
      <td><%= photo.name %> &nbsp;</td>
      <td><%= link_to "View", [:admin, photo] %> &nbsp;</td>
      <td><%= link_to "Edit", edit_admin_photo_path(photo) %></td>
    </tr>
  <% end %>
</table>

<br />
<p><%= link_to 'New photo', new_admin_photo_path %></p>
