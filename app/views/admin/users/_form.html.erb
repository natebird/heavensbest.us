<%= error_messages_for :user %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :login, 'Username' %>
<%= f.text_field :login %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :phone %>
<%= f.text_field :phone %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation, 'Password again' %>
<%= f.password_field :password_confirmation %>

<%= submit_or_cancel %>
